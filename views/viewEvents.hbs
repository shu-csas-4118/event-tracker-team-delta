
<style>
    body {background-color: white}
</style>

<body onload = "funkyJams()" >
    <p><font color="black"><big><u><center> Below contains all available events </center></u></big></font></p>
    <div id = "eves"> {{events}} </div>

    <script>
        function funkyJams(){
            stuff = document.getElementById("eves").innerHTML;
            newArray = stuff.split(",");
            var html='';
            for (var i=0; i<=newArray.length; i++) {
                html+= '<p></p><a href= "/event/viewEvent/'+newArray[i]+'">' +newArray[i]+ '</a>';
            }
            document.getElementById("eves").innerHTML= html;
        }

    </script>
</body>

